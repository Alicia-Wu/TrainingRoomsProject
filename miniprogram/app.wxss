/**app.wxss**/
.container {
  position: absolute;
  top: 100rpx;
  bottom: 0;
  left: 0;
  right: 0;

  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  box-sizing: border-box;
} 


.table {
  border: 0px solid darkgray;
  font-size: 14px;
}
.tr {
  display: flex;
  width: 100%;
  justify-content: center;
  height: 2rem;
  align-items: center;
}
.td {
    width:40%;
    justify-content: center;
    text-align: center;
}
.bg-w{
  background: snow;
}
.bg-g{
  background: #E6F3F9;
}
.th {
  width: 40%;
  justify-content: center;
  background: #2d8cf0;
  color: #fff;
  display: flex;
  height: 2rem;
  align-items: center;
}
.th.th1{
  width: 20%;
}

.td.td1{
  width: 20%;
}